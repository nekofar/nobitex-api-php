{
  "name": "nekofar/nobitex",
  "description": "PHP wrapper for the Nobitex API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Milad Nekofar",
      "email": "milad@nekofar.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "ext-json": "*",
    "psr/http-message": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/httplug": "^1.0",
    "php-http/message-factory": "^1.0",
    "php-http/discovery": "^1.0",
    "netresearch/jsonmapper": "dev-master"
  },
  "require-dev": {
    "php": "^7.2",
    "ext-json": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "dev-master",
    "object-calisthenics/phpcs-calisthenics-rules": "^3.4@dev",
    "phpcompatibility/php-compatibility": "dev-master",
    "phpmd/phpmd": "dev-master",
    "phpunit/phpunit": "^8.3@dev",
    "squizlabs/php_codesniffer": "^3.0@dev",
    "php-http/mock-client": "^1.0",
    "php-http/message": "^1.0",
    "guzzlehttp/psr7": "^1.0",
    "php-http/curl-client": "^1.0",
    "vlucas/phpdotenv": "^3.4@dev"
  },
  "autoload": {
    "psr-4": {
      "Nekofar\\Nobitex\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nekofar\\Nobitex\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "phpcs": "vendor/bin/phpcs"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
